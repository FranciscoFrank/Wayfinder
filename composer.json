{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "package",
            "package": {
                "name": "mably/slick",
                "version": "1.8.2",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/mably/slick.git",
                    "type": "git",
                    "reference": "topic/jquery4"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "fonticonpicker/fonticonpicker",
                "version": "3.1.1",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/fontIconPicker/fontIconPicker.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "bower-asset/lazysizes": "^5.3",
        "composer/installers": "^2.3",
        "cweagans/composer-patches": "^1.7",
        "drupal/address": "^2.0",
        "drupal/admin_toolbar": "^3.5",
        "drupal/ai": "^1.0",
        "drupal/ai_provider_mistral": "^1.0@beta",
        "drupal/ai_vdb_provider_milvus": "^1.0@beta",
        "drupal/allowed_formats": "^3.0",
        "drupal/anonymous_login": "^2.2",
        "drupal/antibot": "^2.0",
        "drupal/automatic_updates": "^3.1",
        "drupal/backup_migrate": "^5.1",
        "drupal/better_exposed_filters": "^7.0",
        "drupal/bootstrap5": "^4.0",
        "drupal/bpmn_io": "^2.0",
        "drupal/cheeseburger_menu": "5.1.x-dev@dev",
        "drupal/ckeditor5_plugin_pack": "^1.2",
        "drupal/ckeditor5_premium_features": "^1.3",
        "drupal/coder": "^8.3",
        "drupal/commerce": "^3.0",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/core-composer-scaffold": "^11.1",
        "drupal/core-project-message": "^11.1",
        "drupal/core-recommended": "^11.1",
        "drupal/country": "^2.1",
        "drupal/dashboard": "^2.0",
        "drupal/devel": "^5.3",
        "drupal/eca": "^2.1",
        "drupal/facets": "^3.0",
        "drupal/fontawesome_menu_icons": "^3.0",
        "drupal/gin": "^4.0",
        "drupal/gin_everywhere": "^1.2",
        "drupal/gin_login": "^2.1",
        "drupal/gin_toolbar": "^2.0",
        "drupal/gin_type_tray": "^1.0@RC",
        "drupal/group": "^3.3",
        "drupal/htmlmail": "^4.0@beta",
        "drupal/languageicons": "^2.0@beta",
        "drupal/layout_custom_section_classes": "^2.0",
        "drupal/lazy": "^4.0",
        "drupal/libraries": "^4.0",
        "drupal/login_emailusername": "^3.0",
        "drupal/masquerade": "^2.0",
        "drupal/maxlength": "^3.1",
        "drupal/metatag": "^2.1",
        "drupal/micon": "^2.1",
        "drupal/paragraphs": "^1.18",
        "drupal/pathauto": "^1.13",
        "drupal/project_browser": "^2.0@beta",
        "drupal/recaptcha_v3": "^2.0",
        "drupal/redirect": "^1.11",
        "drupal/responsive_image_preload": "^2.0",
        "drupal/simple_gmap": "^4.0",
        "drupal/slick": "^3.0",
        "drupal/slick_views": "^3.0",
        "drupal/smtp": "^1.4",
        "drupal/social_media_links": "^2.10",
        "drupal/sodium": "^3.0",
        "drupal/starrating": "^4.1@alpha",
        "drupal/statistics": "^1.0",
        "drupal/svg_image_field": "^2.3",
        "drupal/tfa": "^1.9",
        "drupal/token": "^1.15",
        "drupal/token_or": "^2.3",
        "drupal/twig_tweak": "^3.4",
        "drupal/type_tray": "^1.3",
        "drupal/typed_data": "^2.1",
        "drupal/views_data_export": "^1.5",
        "drupal/webform": "^6.3@beta",
        "drush/drush": "^13.3",
        "fonticonpicker/fonticonpicker": "^3.1",
        "mably/slick": "^1.8",
        "npm-asset/fonticonpicker--fonticonpicker": "^3.1",
        "oomphinc/composer-installers-extender": "^2.0",
        "tubalmartin/cssmin": "^4.1",
        "wikimedia/composer-merge-plugin": "^2.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "RC",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true,
            "wikimedia/composer-merge-plugin": true,
            "cweagans/composer-patches": true,
            "oomphinc/composer-installers-extender": true
        },
        "sort-packages": true
    },
    "extra": {
        "patches": {
            "drupal/lazy": {
                "Lazy | Too few arguments": "https://git.drupalcode.org/project/lazy/-/merge_requests/18.patch"
            },
           "drupal/htmlmail": {
               "HTML Mail | Too few arguments": "https://git.drupalcode.org/project/htmlmail/-/merge_requests/11.patch"
           },
           "drupal/admin_toolbar": {
               "Admin Toolbar | Missing 'Source'": "https://git.drupalcode.org/project/admin_toolbar/-/merge_requests/117.patch"
           },
            "drupal/anonymous_login": {
                "Anonymous login | Argument must be of type array": "https://git.drupalcode.org/project/anonymous_login/-/merge_requests/14.patch"
            },
            "drupal/gin": {
                "Gin | Relabel Enable overrides checkbox on user edit form to Override the admin theme options": "https://git.drupalcode.org/project/gin/-/merge_requests/308.patch"
            }
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "merge-plugin": {
            "include": [
                "web/modules/contrib/webform/composer.libraries.json"
            ]
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:npm-asset",
                "vendor:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "recipes/{$name}": [
                "type:drupal-recipe"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/installing-drupal",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "installer-types": [
            "npm-asset"
        ]
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.12"
    }
}
